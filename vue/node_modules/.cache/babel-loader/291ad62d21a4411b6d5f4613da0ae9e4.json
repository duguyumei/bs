s.isMax = false;\n      }\n    },\n    //上传失败的时候会出现Bug，故在上传失败的时候也进行一下判断\n    error: function (err, file, fileList) {\n      this.$message.error('上传失败');\n\n      if (fileList.length >= 1) {\n        this.isMax = true;\n      } else {\n        this.isMax = false;\n      }\n    },\n    submitForm: function () {\n      this.form_avatar = JSON.parse(JSON.stringify(this.form)); //深拷贝,独立对象\n\n      request.put(\"/user/updateMarchant\").then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible = false;\n          sessionStorage.setItem('data', JSON.stringify(this.from));\n          this.mounted();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.message\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA2GI,OAAOA,OAAP,MAAoB,gBAApB;AAEA,eAAe;AACXC,MAAI,EAAE,gBADK;;AAEXC,MAAI,GAAE;AACF,WAAM;AACFC,UAAI,EAAC,EADH;AAEFC,iBAAW,EAAC,EAFV;AAGFC,WAAK,EAAC,KAHJ;AAIFC,mBAAa,EAAC,KAJZ;AAKFC,WAAK,EAAC,KALJ;AAKU;AACZC,cAAQ,EAAC;AANP,KAAN;AAQH,GAXU;;AAYXC,SAAO,EAAE,YAAY;AACjB,QAAIC,OAAM,GAAIC,IAAI,CAAC,MAAMC,cAAc,CAACC,OAAf,CAAuB,MAAvB,CAAN,GAAuC,GAAxC,CAAlB;AACA,SAAKV,IAAL,GAAYO,OAAZ;AACH,GAfU;AAgBXI,SAAO,EAAC;AACJC,WAAO,EAAC,YAAY;AAChB,WAAKT,aAAL,GAAqB,IAArB;AACH,KAHG;;AAIJ;AACAU,sBAAkB,CAACC,GAAD,EAAMC,IAAN,EAAYV,QAAZ,EAAsB;AACpC,WAAKL,IAAL,CAAUgB,GAAV,GAAgBF,GAAG,CAACf,IAApB;AACH,KAPG;;AAQJ;AACAkB,sBAAkB,EAAE,UAAUF,IAAV,EAAgB;AAChC,YAAMG,KAAI,GAAIH,IAAI,CAACI,IAAL,KAAc,YAA5B;AACA,YAAMC,KAAI,GAAIL,IAAI,CAACI,IAAL,KAAc,WAA5B;AACA,YAAME,IAAG,GAAKH,KAAI,IAAKE,KAAvB,CAHgC,CAGoC;;AACpE,YAAME,MAAK,GAAIP,IAAI,CAACQ,IAAL,GAAY,IAAZ,GAAmB,IAAnB,GAA0B,CAAzC,CAJgC,CAIoC;;AACpE,YAAMC,MAAK,GAAI,IAAIC,OAAJ,CAAY,UAAUC,OAAV,EAAmBC,MAAnB,EAA2B;AAClD,YAAIC,KAAI,GAAI,GAAZ;AACA,YAAIC,MAAK,GAAI,GAAb;;AACA,YAAIC,IAAG,GAAIC,MAAM,CAACC,GAAP,IAAcD,MAAM,CAACE,SAAhC;;AACA,YAA