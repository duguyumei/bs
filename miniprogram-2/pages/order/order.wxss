.container{
  padding: 0;
  margin-top: 20rpx;
  /* background-color: gray; */
  padding-left: 140rpx;
  position: relative;
  margin-bottom: 30rpx;
  /* height: 10rpx; */
}
.dish_name{
  word-break:break-all;
  overflow: hidden;
  /* background-color: greenyellow; */
